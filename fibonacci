#include <iostream>
#include "conio.h"

void fibonacci(int n);

using namespace std;

void main()
{
	int a;

	cout<<"Escriba el numero de elementos de la secuencia: ";
	cin>>a;

	fibonacci(a);

	getch();

}
void fibonacci(int n)
{
	int fib=1,anterior=1,sumar,actual;
		if(n==1){
			cout<<0<<" -- ";}
		else{
			if(n>=2){
				cout<<0<<" -- ";
				cout<<1<<" -- ";}
		}
		for(int i=3;i<=n;i++)
		{	
			cout<<fib<<" -- ";
			actual=fib;
			fib=fib+anterior;
			anterior=actual;

		}

}
